  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "analyzer": "cross-env BUILD_TYPE=analyzer node scripts/build.js",
    "build:prod": "cross-env NODE_ENV=production BASE_URL=/vue3/ webpack --mode production --progress --config config/webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development BASE_URL=/ webpack serve --mode development --progress --config config/webpack.dev.config.js",
    "build:test": "cross-env NODE_ENV=production BASE_URL=/vue3/ BUILD=test webpack --mode production --progress --config config/webpack.prod.config.js",
    "prepare": "husky install",
    "lint": "eslint --ext .js,.vue,.ts,.tsx src",
    "lint-fix": "eslint --fix --ext .js,.vue,.ts,.tsx src"
  },
  "author": "TangHao",
  "homepage": "https://www.tanghaoo.com/",
